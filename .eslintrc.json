{
  "env": {
    "es6": true,
    "node": true
  },
  "extends": ["prettier"],
  "plugins": ["prettier"],
  "parserOptions": {
    "ecmaVersion": 8
  },
  "rules": {
    "no-await-in-loop": "off",
    "no-underscore-dangle": "off",
    "no-console": "off",
    "max-len": "off",
    "consistent-return": "off",
    "no-nested-ternary": "off",
    "camelcase": "off",
    "prettier/prettier": "error"
  }
}
